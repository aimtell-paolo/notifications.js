{
  "name": "notificationsjs",
  "version": "0.1.0",
  "description": "Coming soon...",
  "main": "lib/notifications.js",
  "scripts": {
    "webpack-dev": "webpack -w --config webpack.dev.js",
    "example-dev": "live-server --port=8001",
    "webpack-prod": "webpack --progress --config webpack.prod.js",
    "build": "npm run webpack-prod && cp src/style.css lib",
    "prepublish": "npm run build",
    "test": "ava --tap | tap-dot",
    "test-watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pusher-community/notifications.js.git"
  },
  "keywords": [
    "notifications",
    "pusher",
    "notify"
  ],
  "author": "Jack Franklin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pusher-community/notifications.js/issues"
  },
  "homepage": "https://github.com/pusher-community/notifications.js#readme",
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "6.8.0",
    "doubler": "0.6.0",
    "jsdom": "8.5.0",
    "live-server": "^1.0.0",
    "object-assign": "4.1.0",
    "tap-dot": "1.0.5",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "animate.css": "3.5.1"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "babel": "inherit",
    "verbose": true,
    "require": [
      "babel-register",
      "./tests/jsdom.js"
    ]
  }
}
